{
    "name": "laravel_livewire_shop/laravel_livewire_shop",
    "description": "A complete e-commerce solution for Laravel with Livewire",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "gnakale",
            "email": "gnakaleroland@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "livewire/livewire": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelLivewireShop\\LaravelLivewireShop\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelLivewireShop\\LaravelLivewireShop\\ShopServiceProvider"
            ],
            "aliases": {
                "Cart": "LaravelLivewireShop\\LaravelLivewireShop\\Facades\\Cart"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
