{
    "name": "votre-namespace/laravel-livewire-shop",
    "description": "A complete e-commerce solution for Laravel with Livewire",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "livewire/livewire": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "VotreNamespace\\LaravelLivewireShop\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VotreNamespace\\LaravelLivewireShop\\ShopServiceProvider"
            ],
            "aliases": {
                "Cart": "VotreNamespace\\LaravelLivewireShop\\Facades\\Cart"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
